{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ],
    "functions": {
        "api/**.[jt]s": { "includeFiles": "node_modules/is-word/dictionary/**" }
    }
}
